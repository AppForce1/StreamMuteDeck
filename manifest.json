{
   "content_scripts": [ {
      "js": [ "contentScript.js" ],
      "matches": [ "https://stream-calls-dogfood.vercel.app/*" ],
      "run_at": "document_idle"
   } ],
   "description": "This extension discovers and controls Google Meet calls and passes on the status to MuteDeck. Get MuteDeck from https://mutedeck.com",
   "host_permissions": [ "https://stream-calls-dogfood.vercel.app/*" ],
   "icons": {
      "128": "images/logo-128.png",
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png"
   },
   "manifest_version": 3,
   "name": "MuteDeck Stream Meet Extension",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
